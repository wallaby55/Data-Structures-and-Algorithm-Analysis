
package Chapter1;
import java.util.*;

public class Main {

    public static void main(String[] args) {
        MyClass<Integer> obj = new MyClass<>(222);
        MyClass<Double> obj2 = new MyClass<>(435.22);
        Person x = new Person("shenaya");
        MyClass<Person> obj3 = new MyClass<>(x);

        obj.showType();
        obj2.showType();
        obj3.showType();
        x.showName();
    }
}

